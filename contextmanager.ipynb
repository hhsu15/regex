   "source": [
    "from contextlib import contextmanager\n",
    "\n",
    "@contextmanager\n",
    "def my_context(name):\n",
    "    print(name)\n",
    "    try:\n",
    "        yield\n",
    "    finally:\n",
    "        print(\"out of context\")\n",
    "        \n",
    "with my_context(\"shin\") as mc:\n",
    "    print(\"in the context\")\n",
    "    pass"
   ]
